//teleporter effects (idea taken from telestecki)

textures/dk_2012/teleglow01
{
	deformVertexes autosprite
	cull disable
     	surfaceparm	trans
     	nomipmaps

	{
		clampmap textures/dk_2012/teleglow01.tga
		blendfunc gl_one gl_one
		tcMod stretch sawtooth 1 -.9 .5 1.15
		tcMod rotate 15
		rgbGen wave sawtooth .5 .5 .25 1.15
	}
	{
		clampmap textures/dk_2012/teleglow01.tga
		blendfunc gl_one gl_one
		tcMod stretch sawtooth 1 -.9 .5 1.5
		tcMod rotate -60
		rgbGen wave sawtooth .75 .5 .5 1.5
	}
}


