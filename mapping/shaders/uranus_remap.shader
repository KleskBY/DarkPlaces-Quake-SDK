//gfx/2d/numbers/zero_32b (RemapShader)
uranus_remap/gfx/2d/numbers/_zero_32b
{
	nopicmip
	{
		map remap_fx/gfx/2d/numbers/zero_fx.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbgen vertex
	}
      {
		map remap_fx/gfx/2d/numbers/zero_fx_glow.tga
		blendFunc add
	}

}

