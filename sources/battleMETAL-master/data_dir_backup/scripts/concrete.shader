textures/concrete/concrete_a
{
	qer_editorimage textures/concrete/concrete_a.tga
	q3map_globaltexture

	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/concrete/concrete_a.tga
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

